docker exec -it broker /bin/bash

kafka-topics --create --topic organizations_topic --bootstrap-server broker:29092 --partitions 1 --replication-factor 1

kafka-console-producer --broker-list broker:29092 --topic my_topic


kafka-console-consumer --bootstrap-server broker:29092 --topic eur_topic --from-beginning

kafka-topics --bootstrap-server  broker:9092 --describe --topic users
